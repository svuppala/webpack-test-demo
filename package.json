{
  "name": "webpack-test-demo",
  "version": "1.0.0",
  "description": "A sample webpack karma cypress project",
  "private": true,
  "scripts": {
    "lint": "eslint ./src --fix",
    "start": "npm run lint && webpack-dev-server --open",
    "watch": "webpack --watch",
    "test:unit": "karma start --single-run --no-auto-watch",
    "test:watch": "karma start --no-single-run --auto-watch",
    "cypress:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "concurrently \"webpack-dev-server\" \"cypress run\" -k -s first",
    "test": "npm run lint && npm run test:unit & npm run test:e2e",
    "build": "npm run test && webpack --config ./webpack.config.prod.js --branch integration",
    "build:dev": "npm run test && webpack",
    "deploy": "npm run build"
  },
  "keywords": [],
  "author": "Srivani Vuppala",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "cypress": "^3.1.3",
    "del": "^2.2.0",
    "eslint": "^5.10.0",
    "eslint-config-auspost": "^1.6.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.3.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.0-rc.5",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.11",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "zip-webpack-plugin": "^3.0.0"
  }
}
